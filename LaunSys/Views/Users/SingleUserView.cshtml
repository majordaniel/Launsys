
@{
    ViewBag.Title = "SingleUserView";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <img src="~/Content/logo.jpg" width="100" ; height="70" style="align-content:center" />
</div>

<div>
    <h4>Individual Client Details</h4><br>
    <table class="table table-striped">

        @if (ViewBag.SingleUserDetail != null)
        {
            foreach (var item in ViewBag.SingleUserDetail)
            {

                <tr>
                    <td><b>Email: </b></td>
                    <td>@item.Email</td>
                </tr>
                 <tr>
                    <td><b>Password: </b></td>
                    <td>@item.Password</td>
                </tr>
                <tr>
                    <td><b>Role: </b></td>
                    <td>@item.Rolename</td>
                </tr>
                <tr>
                    <td><b>Branch: </b></td>
                    <td>@item.Branchname</td>
                </tr>
              

            }
        }

    </table>
</div>







